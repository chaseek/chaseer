                <artifactId>adk-de-api</artifactId>
                <version>1.0.0</version>
            </dependency>

            <!-- device-interactive-common通用抽取 -->
            <dependency>
                <groupId>com.adkfp</groupId>
                <artifactId>connect-common</artifactId>
