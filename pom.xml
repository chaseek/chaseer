            </dependency>

            <!-- common-access通用抽取 -->
            <dependency>
                <groupId>com.adkfp</groupId>
                <artifactId>common-access</artifactId>
                <version>${adkfp.version}</version>
            </dependency>

            <!-- lift-common通用抽取 -->
            <dependency>
                <groupId>com.adkfp</groupId>
                <artifactId>lift-common</artifactId>
                <version>${adkfp.version}</version>
            </dependency>

            <!-- lift-device抽取 -->
            <dependency>
                <groupId>com.adkfp</groupId>
                <artifactId>adkfp-lift-device</artifactId>
                <version>${adkfp.version}</version>
            </dependency>

            <!-- ac18远程调用接口 -->
            <dependency>
