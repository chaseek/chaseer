            </dependency>

            <!-- ADKFP web通用模块 -->
            <dependency>
                <groupId>com.adkfp</groupId>
                <artifactId>common-web</artifactId>
                <version>${adkfp.version}</version>
            </dependency>

            <!-- ADKFP web 和DB 依赖管理 -->
            <dependency>
                <groupId>com.adkfp</groupId>
                <artifactId>common-web-db</artifactId>
                <version>${adkfp.version}</version>
