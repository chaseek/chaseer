            <dependency>
                <groupId>com.adkfp</groupId>
                <artifactId>common-data-sync</artifactId>
                <version>${adkfp.version}</version>
            </dependency>

            <!-- interactive-common通用抽取 -->
            <dependency>
                <groupId>com.adkfp</groupId>
